<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.db.chaomaxs.companyservice.dal.mysql.mapper.interview.CompanyUserInterviewMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.db.chaomaxs.companyservice.dal.mysql.dataobject.interview.CompanyUserInterviewDO"
               id="BaseResultMap">
        <result property="id" column="id"/>
        <result property="companyUserId" column="company_user_id"/>
        <result property="positionId" column="position_id"/>
        <result property="userId" column="user_id"/>
        <result property="interviewAddress" column="interview_address"/>
        <result property="interviemTime" column="interviem_time"/>
        <result property="positionName" column="position_name"/>
        <result property="mobile" column="mobile"/>
        <result property="status" column="status"/>
        <result property="isDeleted" column="is_deleted"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>
    <select id="getByCompanyUserIdAndUserId" resultMap="BaseResultMap">
        SELECT id,interviem_time,interview_address,mobile,position_id,user_id
        from company_user_interview
        where company_id=#{companyId} and user_id=#{userId} and company_user_id=#{companyUserId} and is_deleted=0
    </select>
</mapper>
