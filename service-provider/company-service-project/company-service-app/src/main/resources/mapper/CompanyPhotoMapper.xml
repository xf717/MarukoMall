<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.db.chaomaxs.companyservice.dal.mysql.mapper.company.CompanyPhotoMapper">

  <!-- 可根据自己的需求，是否要使用 -->
  <resultMap type="com.db.chaomaxs.companyservice.dal.mysql.dataobject.company.CompanyPhotoDO"
    id="BaseResultMap">
    <result property="id" column="id"/>
    <result property="companyId" column="company_id"/>
    <result property="photoUrl" column="photo_url"/>
    <result property="deleted" column="is_deleted"/>
    <result property="createTime" column="create_time"/>
    <result property="createBy" column="create_by"/>
    <result property="updateTime" column="update_time"/>
    <result property="updateBy" column="update_by"/>
  </resultMap>

  <insert id="batchInsert">
    insert into company_photo(company_id,photo_url) values
    <foreach collection="list" separator="," item="item" index="">
      (
      #{item.companyId},
      #{item.photoUrl}
      )
    </foreach>
  </insert>

</mapper>
