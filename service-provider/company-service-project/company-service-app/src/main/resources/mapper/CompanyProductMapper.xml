<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.db.chaomaxs.companyservice.dal.mysql.mapper.company.CompanyProductMapper">

  <!-- 可根据自己的需求，是否要使用 -->
  <resultMap type="com.db.chaomaxs.companyservice.dal.mysql.dataobject.company.CompanyProductDO"
    id="BaseResultMap">
    <result property="id" column="id"/>
    <result property="companyId" column="company_id"/>
    <result property="logoUrl" column="logo_url"/>
    <result property="productName" column="product_name"/>
    <result property="deleted" column="is_deleted"/>
    <result property="createTime" column="create_time"/>
    <result property="createBy" column="create_by"/>
    <result property="updateTime" column="update_time"/>
    <result property="updateBy" column="update_by"/>
  </resultMap>

  <insert id="batchInsert">
    insert into company_product(company_id, logo_url, product_name) values
    <foreach collection="list" separator="," item="item" index="">
      (#{item.companyId},
      #{item.logoUrl},
      #{item.productName}
      )
    </foreach>
  </insert>
</mapper>
